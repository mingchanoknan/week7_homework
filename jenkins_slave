pipeline {
    agent any
    parameters {
        string(name: 'NAME', defaultValue: 'Tuchsanai', description: 'Enter your name')
        string(name: 'Ages', defaultValue: '35', description: 'Enter your age')
    }
    stages {
        stage('Example') {
            steps {
                sh "python myscript.py ${NAME} --age ${Ages}"
                
            }
        }
    }
}
