pipeline {
    agent any

    stages {
        
        stage('Welcome to master'){
            steps{
                echo 'Start Program'
            }
        }

        stage('Tigger the Slave Jobs') {
            steps {
                props = readJSON file: "${env.WORKSPACE}/data.json"

                echo 'Start Trigger to send name and age to slave'
                build job: 'slave', parameters: [string(name: 'NAME', value: "${props.name}"), string(name: 'Ages', value: "${props.name}")]
            }
        }
    }
}
